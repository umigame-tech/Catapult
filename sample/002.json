{
  "project_name": "data_types",
  "sealed_prefix": "admin",
  "entities": [
    {
      "name": "person",
      "authenticatable": true,
      "attributes": [
        {
          "name": "name",
          "type": "username"
        },
        {
          "name": "email",
          "type": "email",
          "loginKey": true
        },
        {
          "name": "password",
          "type": "password"
        },
        {
          "name": "tel",
          "type": "tel"
        },
        {
          "name": "age",
          "type": "integer",
          "rules": {
            "min": 10,
            "max": 80
          }
        },
        {
          "name": "is_admin",
          "type": "boolean"
        },
        {
          "name": "birthday",
          "type": "date"
        },
        {
          "name": "registered_at",
          "type": "datetime"
        },
        {
          "name": "start_time",
          "type": "time"
        },
        {
          "name": "weight",
          "type": "decimal"
        },
        {
          "name": "description",
          "type": "text"
        }
      ]
    }
  ]
}
